[run]
source =
    scripts
    .
omit =
    tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */.venv/*
    skills-library/*
    setup.py

[report]
precision = 2
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
