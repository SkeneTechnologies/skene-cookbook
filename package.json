  "bin": {
    "skills-directory": "./bin/skills-directory.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.cjs",
    "install:skills": "node bin/skills-directory.js install --target all",
    "install:cursor": "node bin/skills-directory.js install --target cursor",
    "install:claude": "node bin/skills-directory.js install --target claude",
    "status": "node bin/skills-directory.js status",
    "verify:metrics": "node scripts/sync_metrics.cjs",
    "pretest": "npm run verify:metrics",
    "lint": "npm run lint:js && npm run lint:format",
    "lint:js": "eslint \"bin/**/*.js\" \"scripts/**/*.cjs\"",
    "lint:js:fix": "eslint --fix \"bin/**/*.js\" \"scripts/**/*.cjs\"",
    "lint:format": "prettier --check '**/*.{js,json,md,yml,yaml}'",
    "format": "prettier --write '**/*.{js,json,md,yml,yaml}'",
    "format:check": "prettier --check '**/*.{js,json,md,yml,yaml}'"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "react": {
      "optional": true
    }
