{
  "id": "elg_co_sell_trigger",
  "version": "1.0.0",
  "name": "Co-Sell Motion Trigger",
  "description": "Initiates and manages co-selling workflows with partners for joint opportunities.",
  "domain": "ecosystem",
  "tools": [
    {
      "name": "partner.co_sell_trigger",
      "required": true
    },
    {
      "name": "partner.co_sell_update",
      "required": true
    },
    {
      "name": "partner.get",
      "required": true
    },
    {
      "name": "crm.create_deal",
      "required": false
    },
    {
      "name": "resend.send_email",
      "required": false
    }
  ],
  "exitStates": [
    "deal_created",
    "partner_engaged",
    "handoff_orchestration",
    "idle"
  ],
  "metrics": {
    "primary": "co_sell_win_rate",
    "benchmarks": {
      "excellent": "> 50%",
      "good": "30-50%",
      "needs_improvement": "< 30%"
    }
  },
  "temperature": 0,
  "tags": [
    "ecosystem",
    "co-sell",
    "partner",
    "revenue",
    "community"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with Co Sell Motion Trigger"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  },
  "security_controls": {
    "execution_environment": {
      "type": "sandboxed_container",
      "resource_limits": {
        "cpu_cores": 1,
        "memory_mb": 512,
        "timeout_seconds": 30,
        "disk_mb": 100
      },
      "security": {
        "network_access": false,
        "filesystem": "read_only",
        "user": "non_root",
        "allowed_commands": [
          "node",
          "python3",
          "npm"
        ],
        "blocked_syscalls": [
          "exec",
          "fork",
          "kill"
        ]
      }
    }
  }
}