{
  "id": "scientific/clinicaltrials-database",
  "version": "1.0.0",
  "name": "clinicaltrials-database",
  "description": "Query ClinicalTrials.gov via API v2. Search trials by condition, drug, location, status, or phase. Retrieve trial details by NCT ID, export data, for clinical research and patient matching.",
  "domain": "scientific",
  "source": "external",
  "tools": [],
  "exitStates": [
    "success",
    "failure"
  ],
  "inputSchema": {},
  "outputSchema": {},
  "temperature": 0,
  "tags": [
    "scientific",
    "imported",
    "community"
  ],
  "platforms": {
    "claude": {
      "license": "Unknown",
      "triggers": [
        "help with clinicaltrials database",
        "data analysis",
        "research"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  },
  "security_controls": {
    "execution_environment": {
      "type": "sandboxed_container",
      "resource_limits": {
        "cpu_cores": 1,
        "memory_mb": 512,
        "timeout_seconds": 30,
        "disk_mb": 100
      },
      "security": {
        "network_access": false,
        "filesystem": "read_only",
        "user": "non_root",
        "allowed_commands": [
          "node",
          "python3",
          "npm"
        ],
        "blocked_syscalls": [
          "exec",
          "fork",
          "kill"
        ]
      }
    }
  },
  "skipSchemaValidation": true
}
