{
  "id": "scientific/flowio",
  "version": "1.0.0",
  "name": "flowio",
  "description": "Parse FCS (Flow Cytometry Standard) files v2.0-3.1. Extract events as NumPy arrays, read metadata/channels, convert to CSV/DataFrame, for flow cytometry data preprocessing.",
  "domain": "scientific",
  "source": "external",
  "tools": [],
  "exitStates": [
    "success",
    "failure"
  ],
  "inputSchema": {},
  "outputSchema": {},
  "temperature": 0,
  "tags": [
    "scientific",
    "imported",
    "community"
  ],
  "platforms": {
    "claude": {
      "license": "BSD-3-Clause license",
      "triggers": [
        "help with flowio",
        "data analysis"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  },
  "security_controls": {
    "write_operations": {
      "preview_mode": true,
      "validation": {
        "input_schema": true,
        "business_rules": true,
        "data_integrity": true
      },
      "rollback": {
        "enabled": true,
        "window_seconds": 1800
      },
      "audit": {
        "log_before_state": true,
        "log_after_state": true,
        "track_user": true
      }
    }
  },
  "skipSchemaValidation": true
}
