{
  "id": "scientific/gwas-database",
  "version": "1.0.0",
  "name": "gwas-database",
  "description": "Query NHGRI-EBI GWAS Catalog for SNP-trait associations. Search variants by rs ID, disease/trait, gene, retrieve p-values and summary statistics, for genetic epidemiology and polygenic risk scores.",
  "domain": "scientific",
  "source": "external",
  "tools": [],
  "exitStates": [
    "success",
    "failure"
  ],
  "inputSchema": {},
  "outputSchema": {},
  "temperature": 0,
  "tags": [
    "scientific",
    "imported",
    "community"
  ],
  "platforms": {
    "claude": {
      "license": "Unknown",
      "triggers": [
        "help with gwas database",
        "statistics"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  },
  "security_controls": {
    "write_operations": {
      "preview_mode": true,
      "validation": {
        "input_schema": true,
        "business_rules": true,
        "data_integrity": true
      },
      "rollback": {
        "enabled": true,
        "window_seconds": 1800
      },
      "audit": {
        "log_before_state": true,
        "log_after_state": true,
        "track_user": true
      }
    }
  },
  "skipSchemaValidation": true
}
