{
  "id": "marketing_file_organizer",
  "version": "1.0.0",
  "name": "file-organizer",
  "description": "Intelligently organizes your files and folders across your computer by understanding context, finding duplicates, suggesting better structures, and automating cleanup tasks. Reduces cognitive load and keeps your digital workspace tidy without manual effort.",
  "domain": "marketing",
  "category": "operations",
  "source": "skene-marketing",
  "tools": [],
  "exitStates": [
    "idle",
    "support"
  ],
  "inputSchema": {
    "type": "object",
    "properties": {},
    "required": []
  },
  "outputSchema": {
    "type": "object",
    "properties": {}
  },
  "temperature": 0.3,
  "tags": [
    "marketing",
    "operations",
    "file-organizer",
    "community"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with file organizer"
      ]
    },
    "cursor": {},
    "skeneflow": {
      "tools": [
        {
          "name": "resend.send_email",
          "required": false
        },
        {
          "name": "analytics.track_campaign",
          "required": false
        }
      ]
    }
  },
  "security_controls": {
    "payment_controls": {
      "preview_mode": true,
      "per_transaction_approval": true,
      "amount_limits": {
        "max_per_transaction": 50000,
        "daily_limit": 200000,
        "additional_approval_threshold": 25000
      },
      "rollback": {
        "enabled": true,
        "window_seconds": 3600
      },
      "two_phase_commit": true,
      "audit_logging": "detailed"
    }
  }
}