{
  "id": "elg_eql_scoring",
  "version": "1.0.0",
  "name": "Ecosystem-Qualified Lead Scoring",
  "description": "Identifies and scores Ecosystem-Qualified Leads (EQLs) from partner and integration signals.",
  "domain": "ecosystem",
  "tools": [
    {
      "name": "partner.eql_score",
      "required": true
    },
    {
      "name": "partner.account_map",
      "required": false
    },
    {
      "name": "analytics.query_events",
      "required": false
    },
    {
      "name": "crm.create_deal",
      "required": false
    }
  ],
  "exitStates": [
    "lead_qualification",
    "co_sell_trigger",
    "sales_handoff",
    "idle"
  ],
  "inputSchema": {
    "type": "object",
    "properties": {
      "leadId": {
        "type": "string",
        "description": "Lead identifier to score"
      },
      "partnerId": {
        "type": "string",
        "description": "Partner ID associated with lead"
      },
      "signalType": {
        "type": "string",
        "enum": ["integration_install", "marketplace_listing", "partner_referral", "account_overlap"],
        "description": "Type of ecosystem signal"
      },
      "action": {
        "type": "string",
        "enum": ["score", "get_score", "update_score"],
        "description": "Scoring action"
      }
    },
    "required": ["leadId", "action"]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "eql_score": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      },
      "score_breakdown": {
        "type": "object",
        "properties": {
          "partner_signal": {"type": "number"},
          "integration_signal": {"type": "number"},
          "account_overlap": {"type": "number"}
        }
      },
      "qualification_level": {
        "type": "string",
        "enum": ["high", "medium", "low"]
      },
      "recommended_action": {
        "type": "string"
      }
    }
  },
  "metrics": {
    "primary": "eql_conversion_rate",
    "benchmarks": {
      "excellent": "> 40%",
      "good": "25-40%",
      "needs_improvement": "< 25%"
    }
  },
  "temperature": 0,
  "tags": [
    "ecosystem",
    "eql",
    "scoring",
    "leads",
    "community"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with Ecosystem Qualified Lead Scoring"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  },
  "security_controls": {
    "review_note": {
      "flagged_as": "false_positive",
      "requires_manual_review": true,
      "review_date": "2026-02-05T14:36:53.797305"
    }
  }
}