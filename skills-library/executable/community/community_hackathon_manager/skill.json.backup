{
  "id": "community_hackathon_manager",
  "version": "1.0.0",
  "name": "Hackathon Orchestrator",
  "description": "Plans, manages, and executes community hackathons including registration, team formation, judging, and prizes.",
  "domain": "community",
  "tools": [
    {
      "name": "calendar.create_event",
      "required": true
    },
    {
      "name": "crm.get_contacts",
      "required": true
    },
    {
      "name": "ai.match",
      "required": true
    },
    {
      "name": "ai.score",
      "required": true
    },
    {
      "name": "messaging.send_email",
      "required": true
    },
    {
      "name": "analytics.track_event",
      "required": true
    }
  ],
  "exitStates": [
    "swag_manager",
    "contributor_tracker",
    "case_study_finder",
    "idle"
  ],
  "inputSchema": {
    "type": "object",
    "properties": {
      "hackathonName": {
        "type": "string",
        "description": "Name of the hackathon"
      },
      "theme": {
        "type": "string",
        "description": "Hackathon theme or challenge"
      },
      "startDate": {
        "type": "string",
        "format": "date-time",
        "description": "Hackathon start date"
      },
      "endDate": {
        "type": "string",
        "format": "date-time",
        "description": "Hackathon end date"
      },
      "format": {
        "type": "string",
        "enum": [
          "virtual",
          "in_person",
          "hybrid"
        ],
        "description": "Event format"
      },
      "prizes": {
        "type": "array",
        "items": {
          "type": "object"
        },
        "description": "Prize structure"
      },
      "maxTeamSize": {
        "type": "number",
        "description": "Maximum team size"
      }
    },
    "required": [
      "hackathonName",
      "theme",
      "startDate",
      "endDate"
    ]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "hackathonId": {
        "type": "string"
      },
      "registrations": {
        "type": "number"
      },
      "teamsFormed": {
        "type": "number"
      },
      "submissions": {
        "type": "number"
      },
      "winners": {
        "type": "array",
        "items": {
          "type": "object"
        }
      }
    }
  },
  "metrics": {
    "primary": "submission_rate",
    "benchmarks": {
      "excellent": "> 70%",
      "good": "50-70%",
      "needs_improvement": "< 50%"
    }
  },
  "temperature": 0.2,
  "tags": [
    "hackathon",
    "innovation",
    "competition",
    "teams",
    "community"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with Hackathon Orchestrator"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  }
}
