{
  "id": "meta_gtm_orchestrator",
  "version": "1.0.0",
  "name": "GTM Motion Orchestrator",
  "description": "Coordinates PLG, Sales, and Partner motions for unified go-to-market execution",
  "domain": "meta",
  "tools": [
    {
      "name": "lifecycle.get_segment",
      "required": true
    },
    {
      "name": "lifecycle.get_account_stage",
      "required": true
    },
    {
      "name": "crm.get_deal",
      "required": true
    },
    {
      "name": "crm.get_account",
      "required": true
    },
    {
      "name": "partner.get_overlaps",
      "required": true
    },
    {
      "name": "partner.get_partner_tier",
      "required": false
    },
    {
      "name": "analytics.get_product_usage",
      "required": true
    },
    {
      "name": "ai.recommend_motion",
      "required": true
    },
    {
      "name": "ai.score_intent",
      "required": false
    }
  ],
  "exitStates": [
    "plg_activation",
    "sales_assist",
    "lead_qualification",
    "co_sell_trigger",
    "partner_sourced",
    "idle"
  ],
  "inputSchema": {
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "description": "Target account identifier"
      },
      "currentMotion": {
        "type": "string",
        "enum": [
          "plg",
          "sales",
          "partner",
          "hybrid",
          "unknown"
        ]
      },
      "triggerEvent": {
        "type": "string",
        "description": "Event that initiated orchestration"
      },
      "urgency": {
        "type": "string",
        "enum": [
          "low",
          "medium",
          "high",
          "critical"
        ]
      }
    },
    "required": [
      "accountId"
    ]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "recommendedMotion": {
        "type": "string",
        "enum": [
          "plg_self_serve",
          "sales_assist",
          "sales_led",
          "partner_co_sell",
          "partner_sourced"
        ]
      },
      "motionScore": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      },
      "triggerReason": {
        "type": "string"
      },
      "nextSkill": {
        "type": "string"
      },
      "handoffContext": {
        "type": "object"
      },
      "parallelActions": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  },
  "metrics": {
    "primary": "motion_effectiveness",
    "secondary": [
      "time_to_motion_decision",
      "motion_conversion_rate",
      "cross_motion_handoff_success"
    ],
    "benchmarks": {
      "excellent": "> 80%",
      "good": "60-80%",
      "needs_improvement": "< 60%"
    }
  },
  "orchestrates": [
    "plg/self_serve_expansion",
    "plg/reverse_trial",
    "revops/lead_routing",
    "revops/opportunity_scoring",
    "ecosystem/partner_influenced_revenue",
    "ecosystem/nearbound_signal"
  ],
  "temperature": 0,
  "tags": [
    "meta",
    "orchestration",
    "gtm",
    "plg",
    "sales",
    "partner",
    "experimental"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with GTM Motion Orchestrator"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  }
}
