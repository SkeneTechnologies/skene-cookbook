{
  "id": "meta_land_expand_journey",
  "version": "1.0.0",
  "name": "Land & Expand Journey Manager",
  "description": "Manages the complete land-and-expand journey from PLG acquisition through CS-driven adoption to monetization expansion",
  "domain": "meta",
  "tools": [
    {
      "name": "lifecycle.get_journey_stage",
      "required": true
    },
    {
      "name": "lifecycle.get_account_health",
      "required": true
    },
    {
      "name": "analytics.get_adoption_metrics",
      "required": true
    },
    {
      "name": "analytics.get_usage_trends",
      "required": true
    },
    {
      "name": "monetization.get_current_plan",
      "required": true
    },
    {
      "name": "monetization.get_expansion_opportunity",
      "required": true
    },
    {
      "name": "crm.get_account",
      "required": true
    },
    {
      "name": "ai.predict_expansion",
      "required": true
    },
    {
      "name": "ai.recommend_next_action",
      "required": true
    }
  ],
  "exitStates": [
    "activation_focus",
    "adoption_nurture",
    "expansion_ready",
    "renewal_prep",
    "at_risk_intervention",
    "champion_building"
  ],
  "inputSchema": {
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "description": "Target account identifier"
      },
      "currentStage": {
        "type": "string",
        "enum": [
          "trial",
          "onboarding",
          "activation",
          "adoption",
          "expansion",
          "renewal"
        ]
      },
      "triggerType": {
        "type": "string",
        "enum": [
          "scheduled",
          "event_driven",
          "manual"
        ]
      },
      "eventContext": {
        "type": "object",
        "description": "Context from triggering event"
      }
    },
    "required": [
      "accountId"
    ]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "journeyStage": {
        "type": "string"
      },
      "stageHealth": {
        "type": "string",
        "enum": [
          "healthy",
          "at_risk",
          "blocked",
          "accelerating"
        ]
      },
      "primaryFocus": {
        "type": "string"
      },
      "nextMilestone": {
        "type": "object"
      },
      "recommendedSkills": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "expansionReadiness": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      },
      "blockers": {
        "type": "array",
        "items": {
          "type": "object"
        }
      }
    }
  },
  "metrics": {
    "primary": "net_revenue_retention",
    "secondary": [
      "time_to_value",
      "expansion_rate",
      "stage_progression_velocity"
    ],
    "benchmarks": {
      "excellent": "> 130% NRR",
      "good": "110-130% NRR",
      "needs_improvement": "< 110% NRR"
    }
  },
  "orchestrates": [
    "plg/guided_setup_wizard",
    "plg/milestone_celebration",
    "plg/quick_win_generator",
    "customer_success/onboarding_health",
    "customer_success/adoption_score",
    "customer_success/success_plan_generator",
    "customer_success/time_to_impact",
    "monetization/upgrade_trigger",
    "monetization/consumption_analyzer",
    "monetization/packaging_optimizer"
  ],
  "temperature": 0,
  "tags": [
    "meta",
    "orchestration",
    "land-expand",
    "plg",
    "customer-success",
    "monetization",
    "experimental"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with Land & Expand Journey Manager"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  }
}
