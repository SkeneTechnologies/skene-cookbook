{
  "id": "marketing_social_listening_analyzer",
  "version": "1.0.0",
  "name": "social-listening-analyzer",
  "description": "Analyzes social listening signals to extract insights, match posts to hypotheses, identify trends, and surface opportunities. Goes beyond counting to synthesize what signals mean for campaign strategy. Use when reviewing social listening data.",
  "domain": "marketing",
  "category": "research",
  "source": "skene-marketing",
  "tools": [],
  "exitStates": [
    "idle",
    "support"
  ],
  "inputSchema": {
    "type": "object",
    "properties": {},
    "required": []
  },
  "outputSchema": {
    "type": "object",
    "properties": {}
  },
  "temperature": 0.3,
  "tags": [
    "marketing",
    "research",
    "social-listening-analyzer",
    "community"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with social listening analyzer",
        "reviewing social listening data"
      ]
    },
    "cursor": {},
    "skeneflow": {
      "tools": [
        {
          "name": "resend.send_email",
          "required": false
        },
        {
          "name": "analytics.track_campaign",
          "required": false
        }
      ]
    }
  },
  "security_controls": {
    "data_access": {
      "field_level_control": true,
      "allowed_fields": [
