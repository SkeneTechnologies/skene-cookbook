{
  "id": "anthropic_official/webapp-testing",
  "version": "1.0.0",
  "name": "webapp-testing",
  "description": "Toolkit for interacting with and testing local web applications using Playwright. Supports verifying frontend functionality, debugging UI behavior, capturing browser screenshots, and viewing browser logs.",
  "domain": "anthropic_official",
  "source": "external",
  "tools": [],
  "exitStates": [
    "success",
    "failure"
  ],
  "inputSchema": {},
  "outputSchema": {},
  "temperature": 0,
  "tags": [
    "anthropic_official",
    "imported",
    "verified"
  ],
  "platforms": {
    "claude": {
      "license": "Complete terms in LICENSE.txt",
      "triggers": [
        "help with webapp testing"
      ]
    },
    "cursor": {},
    "skeneflow": {}
  },
  "categories": [
    "marketing",
    "marketing/tools"
  ],
  "security_controls": {
    "execution_environment": {
      "type": "sandboxed_container",
      "resource_limits": {
        "cpu_cores": 1,
        "memory_mb": 512,
        "timeout_seconds": 30,
        "disk_mb": 100
      },
      "security": {
        "network_access": false,
        "filesystem": "read_only",
        "user": "non_root",
        "allowed_commands": [
          "node",
          "python3",
          "npm"
        ],
        "blocked_syscalls": [
          "exec",
          "fork",
          "kill"
        ]
      }
    }
  }
}