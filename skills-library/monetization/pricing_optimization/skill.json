{
  "id": "mon_pricing_optimization",
  "version": "1.0.0",
  "name": "Pricing Optimization",
  "description": "Analyzes pricing effectiveness and recommends optimizations for revenue growth.",
  "domain": "monetization",
  "tools": [
    {
      "name": "analytics.get_metrics",
      "required": true
    },
    {
      "name": "analytics.cohort",
      "required": true
    },
    {
      "name": "stripe.get_subscription",
      "required": false
    },
    {
      "name": "crm.get_deal_velocity",
      "required": false
    }
  ],
  "exitStates": [
    "pricing_test",
    "tier_restructure",
    "value_metric_update",
    "idle"
  ],
  "metrics": {
    "primary": "arpu_growth",
    "benchmarks": {
      "excellent": "> 20% YoY",
      "good": "10-20% YoY",
      "needs_improvement": "< 10% YoY"
    }
  },
  "temperature": 0,
  "tags": [
    "monetization",
    "pricing",
    "optimization",
    "revenue",
    "community"
  ],
  "platforms": {
    "claude": {
      "triggers": [
        "help with Pricing Optimization",
        "pricing",
        "revenue optimization"
      ]
    },
    "cursor": {},
    "skeneflow": {
      "tools": [
        {
          "name": "stripe.get_customer",
          "required": true
        },
        {
          "name": "stripe.list_subscriptions",
          "required": false
        },
        {
          "name": "stripe.create_checkout",
          "required": false
        }
      ]
    }
  }
}
